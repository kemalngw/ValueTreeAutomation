                    <ui:InvokeCode ContinueOnError="{x:Null}" Code="System.Threading.Thread.CurrentThread.CurrentCulture = New System.Globalization.CultureInfo(&quot;en-US&quot;)&#xD;&#xA;' ================== REMOVE ROW 9 KE BAWAH ==================&#xD;&#xA;If dtRaw.Rows.Count &gt; 8 Then&#xD;&#xA;    For i As Integer = dtRaw.Rows.Count - 1 To 8 Step -1&#xD;&#xA;        dtRaw.Rows.RemoveAt(i)&#xD;&#xA;    Next&#xD;&#xA;End If&#xD;&#xA;dtRaw.AcceptChanges()&#xD;&#xA;' ================== FIND HEADER POSITION (Date-based) ==================&#xD;&#xA;Dim idxStartMonth As Integer = -1&#xD;&#xA;Dim tempDate As DateTime&#xD;&#xA;&#xD;&#xA;For i As Integer = 0 To dtRaw.Columns.Count - 1&#xD;&#xA;&#xD;&#xA;    Dim rawValue As String = dtRaw.Rows(0)(i).ToString().Trim()&#xD;&#xA;&#xD;&#xA;    ' Cek apakah cell bisa dianggap sebagai tanggal&#xD;&#xA;    If DateTime.TryParse(rawValue, tempDate) Then&#xD;&#xA;        idxStartMonth = i&#xD;&#xA;        Exit For&#xD;&#xA;    End If&#xD;&#xA;&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;If idxStartMonth = -1 Then&#xD;&#xA;    Throw New Exception(&quot;Tidak ditemukan header berbentuk tanggal pada baris pertama (contoh: YTD 31 MAR 2025)&quot;)&#xD;&#xA;End If&#xD;&#xA;' ================== DETECT MONTH ==================&#xD;&#xA;Try&#xD;&#xA;    If idxStartMonth &lt; 0 Then&#xD;&#xA;        Throw New Exception(&quot;idxStartMonth tidak valid&quot;)&#xD;&#xA;    End If&#xD;&#xA;&#xD;&#xA;    If dtRaw.Rows.Count = 0 Then&#xD;&#xA;        Throw New Exception(&quot;dtRaw tidak punya data&quot;)&#xD;&#xA;    End If&#xD;&#xA;&#xD;&#xA;    For i = idxStartMonth To dtRaw.Columns.Count - 1&#xD;&#xA;        Dim val As String = dtRaw.Rows(0)(i).ToString.Trim&#xD;&#xA;&#xD;&#xA;        If Not String.IsNullOrEmpty(val) Then&#xD;&#xA;            Dim d As DateTime&#xD;&#xA;&#xD;&#xA;            If DateTime.TryParseExact(val,&#xD;&#xA;                {&quot;MMM-yy&quot;,&quot;MMM yyyy&quot;,&quot;MM/yyyy&quot;},&#xD;&#xA;                New System.Globalization.CultureInfo(&quot;en-US&quot;),&#xD;&#xA;                System.Globalization.DateTimeStyles.None, d) Then&#xD;&#xA;&#xD;&#xA;                monthsPilar.Add(Tuple.Create(i, d))&#xD;&#xA;            End If&#xD;&#xA;        End If&#xD;&#xA;    Next&#xD;&#xA;&#xD;&#xA;Catch ex As Exception&#xD;&#xA;    errMsg = ex.Message&#xD;&#xA;End Try&#xD;&#xA;&#xD;&#xA;' ================== CREATE DATATABLE ==================&#xD;&#xA;Dim subMonth = {&quot;Plan&quot;,&quot;Actual&quot;,&quot;Polis&quot;,&quot;CIF&quot;,&quot;Ach%&quot;}&#xD;&#xA;&#xD;&#xA;dtPilar = New DataTable&#xD;&#xA;&#xD;&#xA;dtPilar.Columns.Add(&quot;Name&quot;)&#xD;&#xA;&#xD;&#xA;' === Dynamic Month Headers Pilar ===&#xD;&#xA;For Each m In monthsPilar&#xD;&#xA;    Dim mName As String = m.Item2.ToString(&quot;MMM-yy&quot;)&#xD;&#xA;    For Each h In subMonth&#xD;&#xA;        dtPilar.Columns.Add(&quot;Pilar_&quot; &amp; mName &amp; &quot;_&quot; &amp; h)&#xD;&#xA;    Next&#xD;&#xA;Next&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;' ================== FILL DATA ==================&#xD;&#xA;For r = 1 To dtRaw.Rows.Count - 1&#xD;&#xA;&#xD;&#xA;    Dim nameValue As String = dtRaw.Rows(r)(0).ToString.Trim&#xD;&#xA;    If nameValue = &quot;&quot; Then Continue For&#xD;&#xA;&#xD;&#xA;    Dim pRow = dtPilar.NewRow&#xD;&#xA;&#xD;&#xA;    pRow(&quot;Name&quot;) = nameValue&#xD;&#xA;&#xD;&#xA;    ' ===== PILAR DATA =====&#xD;&#xA;    Dim pi As Integer = 1&#xD;&#xA;&#xD;&#xA;    For Each m In monthsPilar&#xD;&#xA;        For j = 0 To 4&#xD;&#xA;            If m.Item1 + j &lt; dtRaw.Columns.Count Then&#xD;&#xA;                pRow(pi) = dtRaw.Rows(r)(m.Item1 + j).ToString&#xD;&#xA;            End If&#xD;&#xA;            pi += 1&#xD;&#xA;        Next&#xD;&#xA;    Next&#xD;&#xA;&#xD;&#xA;    dtPilar.Rows.Add(pRow)&#xD;&#xA;&#xD;&#xA;Next&#xD;&#xA;" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="500.8,196.8" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
